{
  "name": "errorcounter",
  "displayName": "error404",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onDidChangeActiveTextEditor"
  ],
  "main": "./extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "errorcounter",
          "title": "Errors",
          "icon": "media/error.png"
        }
      ]
  },
    "views": {
      "errorcounter": [
        {
          "id": "errorcounter.errorCount",
          "type": "webview",
          "name": "Error Count",
          "icon": "media/error.png"
        }
      ]
    },
    "commands": [
      {
        "command": "errorcounter.trigger",
        "title": "Trigger Color Chaos"
      },
      {
        "command": "errorcounter.openSidebar",
        "title": "Open Error Counter Sidebar",
        "icon": "media/error.png"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.99.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.23.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
